{
  "RoadRunner": {
    "Enabled": true,
    "Publishers": [
      {
        "Enabled": true,
        "Name": "PoiReportV3",
        "Server": "localhost",
        "Port": 5672,
        "Username": "admin",
        "Password": "admin",
        "VirtualHost": "/",
        "Channels": [
          {
            "Name": "Telecom",
            "Enabled": true,
            "ExchangeName": "poireport",
            "RoutingKey": "telecom",
            "Validations": {
              "CheckMessageSize": true,
              "MaxMessageSize": 20480
            },
            "OnError": {
              "Path": " .../../",
              "AllowBatch": true, "BatchSize": 100, "Delay": 3600
            }
          },
          {
            "Name": "Hardware",
            "Enabled": true,
            "ExchangeName": "poireport",
            "RoutingKey": "hardware",
            "Validations": {
              "CheckMessageSize": true,
              "MaxMessageSize": 20480
            }
          }
        ]
      }
    ]
  }
}